{
  "name": "updatedvendure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "copy": "npx copyfiles -u 1 'admin-ui/dist/**/*' dist",
    "build": "tsc && yarn copy",
    "build:admin": "rimraf admin-ui && npx ts-node src/compile-admin-ui.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "main": "dist/index.js",
    "type": "module"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/lib-storage": "^3.744.0",
    "@bull-board/express": "^6.7.10",
    "@bull-monitor/express": "^5.4.0",
    "@nestjs/apollo": "^12.2.2",
    "@pinelab/vendure-plugin-modify-customer-orders": "^1.6.0",
    "@pinelab/vendure-plugin-stock-monitoring": "^1.5.0",
    "@types/request": "^2.48.12",
    "@vendure/admin-ui-plugin": "3.0.5",
    "@vendure/asset-server-plugin": "^3.0.5",
    "@vendure/core": "3.0.5",
    "@vendure/elasticsearch-plugin": "^3.0.5",
    "@vendure/email-plugin": "3.0.5",
    "@vendure/job-queue-plugin": "^3.1.7",
    "@vendure/migrate-v2": "^0.4.0",
    "aws-sdk": "^2.1692.0",
    "bull-board": "^2.1.3",
    "bullmq": "^5.44.1",
    "copyfiles": "^2.4.1",
    "dotenv": "16.4.5",
    "express": "^4.21.2",
    "mysql2": "^3.14.1",
    "otplib": "^12.0.1",
    "razorpay": "^2.9.6",
    "request": "^2.88.2",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.1",
    "@graphql-codegen/typescript": "4.1.2",
    "@types/aws-sdk": "^0.0.42",
    "@types/luxon": "^3.6.2",
    "@types/react": "18.3.12",
    "@vendure/cli": "3.0.5",
    "@vendure/ui-devkit": "3.0.5",
    "concurrently": "9.0.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "overrides": {
    "@nestjs/core": "^10.4.15",
    "@nestjs/common": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/typeorm": "^10.0.2",
    "graphql": "16.8.1"
  }
}
